   "source": [
    "# 迴圈 (loop) 跳出\n",
    "\n",
    "- `break`: 跳出\n",
    "- `continue`: 跳過"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 2 3 break\n"
     ]
    }
   ],
   "source": [
    "for i in range(1, 11):\n",
    "    if i == 4: break\n",
    "    print(i, end=\" \")\n",
    "print(\"break\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 2 3 5 6 7 8 9 10 break\n"
     ]
    }
   ],
   "source": [
    "for i in range(1, 11):\n",
    "    if i == 4: continue\n",
    "    print(i, end=\" \")\n",
    "print(\"break\")"
   ]
  }
